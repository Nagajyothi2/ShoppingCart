@model List<Category>

<figure>
    <blockquote class="blockquote">
        <p class="mb-0">This is Category Page. This page would be used by Admin to add any new category for books.</p>

    </blockquote>
    <figcaption class="blockquote-footer">
        This is Admin <cite title="Source Title">Admin roll required</cite>
    </figcaption>
</figure>

<button type="button" class="btn btn-danger">Create Category</button>
<table class="table table-hover">
    <thead>
        <tr>
            <th scope="col">CategoryId</th>
            <th scope="col">Name</th>
            <th></th>
            
        </tr>
    </thead>
    <tbody>
        @foreach(var i in Model)
        {
            <tr class="table-success">
                <td>@i.CategoryId</td>
                <td>@i.Name</td>
                <td>Edit</td>
            </tr>
        }

            
    </tbody>
</table>
